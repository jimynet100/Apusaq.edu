{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@types/node"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "dynamic-form-lib": [
        "dist/dynamic-form-lib"
      ],
      "dynamic-form-lib/*": [
        "dist/dynamic-form-lib/*"
      ],
      "exceljs": [
        "../node_modules/exceljs/dist/exceljs.min.js"
      ]
    }
  }
}